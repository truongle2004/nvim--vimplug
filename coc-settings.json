{
  "colors.enable": true,
  "rust-analyzer.disableProgressNotifications": true,
  "rust-analyzer.diagnostics.enable": false,
  "coc.source.file.shortcut": "File",
  "coc.source.around.shortcut": "?",
  //"suggest.disableKind": true,
  "suggest.completionItemKindLabels": {
    "keyword": "/uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "tsserver.disableAutomaticTypeAcquisition": true,
  "javascript.suggest.jsdoc.generateReturns": true,
  "javascript.showDeprecated": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.suggest.includeCompletionsForImportStatements": true,
  "javascript.suggest.completeJSDocs": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.enabled": true,
  "javascript.suggest.autoImports": true,
  "javascript.suggest.names": true,
  "inlayHint.display": false,
  "languageserver": {
    // C++
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json"],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache",
          "format": "binary"
        },
        "client": {
          "snippetSupport": true
        },
        "index": {
          "onChange": true
        },
        "highlight": {
          "lsRanges": true
        }
      }
    },
    "cmake": {
      "command": "cmake-language-server",
      "filetypes": ["cmake"],
      "rootPatterns": ["build/"],
      "initializationOptions": {
        "buildDirectory": "build"
      }
    }
  },
  //"java.format.enabled": true,
  //"java.jdt.ls.lombokSupport.enabled": false,
  //"java.jdt.ls.vmargs": "-javaagent:D:/main/lombok-1.18.26.jar",
